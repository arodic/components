{
  "name": "@iogui/iogui",
  "version": "0.0.1-rc1",
  "description": "IoGui is a simple web framework for the web.",
  "main": "build/iogui.js",
  "module": "build/iogui.js",
  "type": "module",
  "files": [
    "src",
    "build/*.js",
    "LICENSE",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "docs": "node ./lib/generate-docs.js",
    "lint": "eslint src --fix",
    "rollup": "rollup -c",
    "uglify-io": "uglifyjs build/iogui.js -cmo build/iogui.js --comments",
    "uglify-elements": "uglifyjs build/io-elements.js -cmo build/io-elements.js --comments",
    "uglify": "npm run uglify-io && npm run uglify-elements",
    "build": "eslint src && npm run rollup && npm run uglify",
    "travis": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iogui/io.git"
  },
  "keywords": [
    "io",
    "gui",
    "iogui",
    "iogui"
  ],
  "author": "Aleksandar (Aki) Rodic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iogui/iogui/issues"
  },
  "homepage": "https://iogui.dev",
  "devDependencies": {
    "@rollup/plugin-strip": "^1.3.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.0.0",
    "rollup": "^2.8.2",
    "uglify-es": "^3.3.9",
    "eslint-plugin-html": "^6.0.2",
    "jsdoc-to-markdown": "^5.0.3"
  }
}
